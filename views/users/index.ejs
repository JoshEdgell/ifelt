      <ul>
        <% for (let i = 0; i < users.length ; i++) { %>
          <li><a href="/users/<%=users[i]._id%>"><%=users[i].firstName%></a></li>
        <% } %>
      </ul>
    </div>
    <% include ../../partials/footer.ejs %>
  </body>
</html>
